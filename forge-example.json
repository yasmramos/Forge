{
  "name": "my-awesome-project",
  "version": "1.0.0",
  "sourceDirectory": "src/main/java",
  "outputDirectory": "target/classes",
  "sourcePaths": [
    "src/main/java",
    "src/main/resources"
  ],
  "dependencies": {
    "junit:junit": {
      "version": "5.9.2",
      "type": "maven"
    },
    "org.slf4j:slf4j-api": {
      "version": "2.0.7",
      "type": "maven"
    },
    "ch.qos.logback:logback-classic": {
      "version": "1.4.11",
      "type": "maven"
    },
    "com.google.code.gson:gson": {
      "version": "2.10.1",
      "type": "maven"
    }
  },
  "plugins": {
    "checkstyle": {
      "enabled": true,
      "version": "10.7.0"
    },
    "spotbugs": {
      "enabled": true,
      "version": "4.7.3"
    }
  },
  "buildSettings": {
    "incremental": true,
    "parallel": true,
    "cacheEnabled": true,
    "threads": 8,
    "compiler": "javac",
    "encoding": "UTF-8",
    "debug": true,
    "optimize": true,
    "sourceCompatibility": "11",
    "targetCompatibility": "11"
  }
}